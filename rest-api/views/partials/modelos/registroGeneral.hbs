{{#if registrosGenerales}}
  <table class="table table-bordered table-hover table-dark">
    <thead>
      <tr>
        <th>N.</th>
        <th>Valor Lectura</th>
        <th>Fecha</th>
      </tr>
    </thead>
    <tbody>
      {{#each registrosGenerales}}
        <tr>
          <td>{{@index}}</td>
          <td>{{valor_lectura}}</td>
          <td>{{fecha_lectura}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
  <div>
    <form action="/api/registro-general/algo" method="get">
      <button class="btn btn-primary btn-sm mt-2">Recargar</button>
    </form>

  </div>
{{else}}

  <div class="h-100 d-flex align-items-center justify-content-center">
    <div class="text-center text-white">
      <h1>No hay registros</h1>
      <img src="/icons/empty.svg" class="w-75" />
    </div>
  </div>

{{/if}}